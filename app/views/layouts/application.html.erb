<!DOCTYPE html>
<html>
  <head>
    <title>Editor</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= yield :stylesheets %>
  </head>

  <body>
    <div class="container">
      <%= yield %>
    </div>
    <%= yield :scripts %>
  </body>
</html>
